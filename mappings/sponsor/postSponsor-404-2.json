{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/teams/2/sponsors",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.name =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.country =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.funding >= 0)]" }
    ]
  },
  "response": {
    "status": 404,
    "bodyFileName": "sponsorNotFound.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}