{

  "request": {
    "method": "POST",
    "url": "/teams",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.name =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.country =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.budget >= 0)]" }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "team/postTeam-ok-3.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}