{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/stages/3/climbs",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.name =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.category =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.slope >= 0)]" }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "postClimb-ok-3.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}