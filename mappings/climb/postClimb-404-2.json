{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/stages/2/climbs",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.name =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.category =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.slope >= 0)]" }
    ]
  },
  "response": {
    "status": 404,
    "bodyFileName": "stageNotFound.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}