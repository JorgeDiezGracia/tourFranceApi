{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/stages",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.departure =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.arrival =~ /^(?!\\s*$).+/)]" },
      { "matchesJsonPath": "$[?(@.kilometers >= 0)]" }
    ]
  },
  "response": {
    "status": 201,
    "bodyFileName": "stage/postStage-ok-3.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}